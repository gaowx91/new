.panel{font-size:0.32rem;color:#999;background:#fff;-moz-border-radius:7px;-webkit-border-radius:7px;border-radius:7px;border:1px solid #ddd;padding:0 0.26667rem}.panel .icon-angle-down{display:inline-block;margin-right:0.133333rem;color:#feb900;-moz-transition:all .2s;-o-transition:all .2s;-webkit-transition:all .2s;transition:all .2s}.panel .icon-angle-down.selected{-moz-transform:rotate(180deg);-ms-transform:rotate(180deg);-webkit-transform:rotate(180deg);transform:rotate(180deg);-moz-transform-origin:center center;-ms-transform-origin:center center;-webkit-transform-origin:center center;transform-origin:center center}.panel-heading{height:1.05rem;display:-webkit-flex;display:flex;-webkit-align-items:center;align-items:center}.panel-body{padding:0 0 0.266667rem}.panel-collapse{margin-top:0.266667rem;line-height:1.6;font-size:0.32rem;border-top:1px solid #ddd;padding:0.133333rem 0 0}.panel-collapse .mint-button{margin:0.133333rem 0 0;background:transparent}
