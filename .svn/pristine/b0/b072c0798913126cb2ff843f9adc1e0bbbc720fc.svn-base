e);\r\n\r\n         this.vcNationcode = this.FUNCTION.getUserCookie('vcNationcode');\r\n         let vcMobile = this.FUNCTION.getUserCookie('vcMobile');\r\n         //手机号显示头尾两位数，中间用星号代替\r\n         this.vcMobile = this.FUNCTION.plusXing(vcMobile,2,2);\r\n\r\n        //console.log(this.FUNCTION.getCookie('User_Cookie_Name'));\r\n    },\r\n    methods: {\r\n        sendCode() {\r\n            // if(!this.countrycode){\r\n            //     this.Toast('請選擇國家地區');\r\n            //     return false;\r\n            // }\r\n            let postData={\r\n                openid:this.openid,\r\n                token:this.token,\r\n                countrycode:this.countrycode,\r\n            };\r\n            this.$http({\r\n               method: 'POST',\r\n                url: this.CONFIG.SEND_BIND_FB_CODE_MS, //發送綁定FaceBook用戶驗證短信\r\n                data: this.$qs.stringify(postData),\r\n            }).then(res => {\r\n                 this.Toast(res.data.info);\r\n                if(res.data.status == true) {\r\n                    // let userData=res.data.data;\r\n                }else{\r\n                    if(res.data.errCode == -2){\r\n                        this.$router.go(-1);\r\n                    }\r\n                }\r\n            }).catch(error => {\r\n                this.Toast('todo');\r\n            });\r\n        },\r\n        bindFbUser() {\r\n            // if(!this.countrycode){\r\n            //     this.Toast('請選擇國家地區');\r\n            //     return false;\r\n            // }\r\n            let postData={\r\n                openid:this.openid,\r\n                token:this.token,\r\n                code:this.code,\r\n            };\r\n            this.$http({\r\n               method: 'POST',\r\n                url: this.CONFIG.BIND_FB_USER, //綁定FaceBook用戶\r\n                data: this.$qs.stringify(postData),\r\n            }).then(res => {\r\n                 this.Toast(res.data.info);\r\n                if(res.data.status == true) {\r\n          