.ol-tab-btn{background-image:-webkit-linear-gradient(top, #dbdbdb, #dbdbdb 50%, transparent 50%);background-image:linear-gradient(180deg, #dbdbdb,#dbdbdb 50%,rgba(0,0,0,0) 50%);background-size:120% 1px;background-repeat:no-repeat}.order-list li{background:#fff;margin:0 0 0.4rem;font-size:0.32rem}.order-list li .hd,.order-list li .md,.order-list li .ft{padding:0.266667rem 0.4rem}.order-list li .md{background:#f9f9f9;font-size:0.346667rem}.order-list li .title{font-weight:normal;font-size:inherit;margin:0 0 0.133333rem}.order-list li .title span{margin:0 0.066667rem 0 0}.ol-col{display:-webkit-flex;display:flex;-webkit-justify-content:space-between;justify-content:space-between;-webkit-align-items:center;align-items:center}.ol-tab-btn{background-position:top;padding:0.266667rem 0 0;margin:0.266667rem 0 0;text-align:right}.ol-tab-btn .btn{-moz-border-radius:.53333rem;-webkit-border-radius:.53333rem;border-radius:.53333rem;padding:0.066667rem 0;max-width:2.533333rem;width:2.533333rem;margin:0 0 0 0.133333rem;font-size:inherit}.ol-tab-btn .btn.is-notice{position:relative;border:1px solid #ff3333;color:#ff3333}.ol-num{position:absolute;width:0.42rem;height:0.42rem;top:-0.21rem;right:-.1rem;font-style:normal;background:#ff3333;color:#fff;-moz-border-radius:100%;-webkit-border-radius:100%;border-radius:100%;font-size:0.24rem;display:-webkit-flex;display:flex;-webkit-align-items:center;align-items:center;-webkit-justify-content:center;justify-content:center}
